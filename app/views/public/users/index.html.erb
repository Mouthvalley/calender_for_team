<%= link_to "ログアウト", destroy_user_session_path, method: :delete %>

<div class='container'>
  <div class='row'>
    <div class='col-md-3'>
      <h2>User info</h2>
      <%= render 'public/users/info', user: current_user %>
      <%= link_to my_page_edit_path, class: "btn btn-outline-secondary btn-block" do %>
        <i class="fas fa-user-cog"></i>
      <% end %></br>
      <%= render partial: 'public/searches/search', locals: { url: search_path } %></br>
      <%= render 'public/schedules/form', schedule: @schedule %></br>
    </div>
  </div>
</div>

